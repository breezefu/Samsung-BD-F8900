    <type name="CsrUieInput" resource="TYPE" size="16">
        <!-- Input modes -->
        <value name="CSR_UI_INPUTMODE_AUTO"         value="0"><!-- Key presses are filtered by UI, rest discarded --></value>
        <value name="CSR_UI_INPUTMODE_AUTOPASS"     value="1"><!-- Key presses are filtered by UI, rest sent to application --></value>
        <value name="CSR_UI_INPUTMODE_BLOCK"        value="2"><!-- All key presses are discarded --></value>
        <value name="CSR_UI_INPUTMODE_PASS"         value="3"><!-- All key presses are sent to application --></value>
    </type>

    <type name="CsrUieAction" resource="TYPE" size="16">
