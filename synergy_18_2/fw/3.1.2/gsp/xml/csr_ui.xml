        <value name="CSR_UI_HIGHESTPRIORITY"        value="255"/>
    </type>

    <type name="CsrUieHEadset" resource="TYPE" size="16">
        <!-- Valid values for state of headset in CSR_UI_STATUS_HEADSET_SET_REQ -->
        <value name="CSR_UI_STATUS_HEADSET_PRESERVE"                    value="-1"><!-- Retain the current state --></value>
        <value name="CSR_UI_STATUS_HEADSET_NOT_PRESENT"                 value="0"><!-- Set to not present --></value>
        <value name="CSR_UI_STATUS_HEADSET_PRESENT"                     value="1"><!-- Set to present (no indicator) --></value>
        <value name="CSR_UI_STATUS_HEADSET_PRESENT_INDICATOR(value)"    value="(2 + value)"><!-- Set to present with indicator (range: 0-100) --></value>
    </type>

