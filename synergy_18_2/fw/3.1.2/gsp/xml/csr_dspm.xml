        </field>
    </type>

    <type name="CsrDspmDeactivateCfm" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmDeactivateCfm">
    </type>

    <type name="CsrDspmDeactivateInd" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmDeactivateInd">
    </type>

    <type name="CsrDspmStreamCpOpenReq" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmStreamCpOpenReq">
