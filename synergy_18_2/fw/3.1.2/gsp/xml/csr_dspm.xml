    <type name="CSR_DSPM_Signal_Id" partial="true" resource="TYPE" size="16" abbr="Sig">
        <value name="CsrDspmActivateReq"            value="0x0000"/>
        <value name="CsrDspmDeactivateReq"          value="0x0001"/>
        <value name="CsrDspmStreamCpOpenReq"        value="0x0002"/>
