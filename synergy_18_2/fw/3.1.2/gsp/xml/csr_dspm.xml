        </field>
    </type>

    <type name="CsrDspmStreamScoEnableReq" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmStreamScoEnableReq">
        <field name="qid">
            <!-- Application queue -->
            <type>CsrSchedQid</type>
        </field>
        <field name="enable">
            <!-- Set to TRUE to enable SCO streams, FALSE to disable -->
            <type>CsrBool</type>
        </field>
    </type>

    <type name="CsrDspmStreamScoEnableCfm" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmStreamScoEnableCfm">
        <field name="result">
            <!-- Result of operation -->
            <type>CsrResult</type>
        </field>
