<?xml version="1.0"?>

<definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="..\hostio\hip.xsd">

    <resource name="FILE"/>
    <resource name="CSR_DSPM_SIGNALS" discriminant="CSR_DSPM_Signal_Id"/>

    <type name="csr_types_xml" resource="FILE" file="csr_types.xml"/>
    <type name="csr_result_xml" resource="FILE" file="csr_result.xml"/>
    <type name="csr_mblk_xml" resource="FILE" file="csr_mblk.xml"/>

    <type name="CsrDspmResult" resource="TYPE" type="CsrResult" const="1">
        <!-- Result Codes -->
        <value name="CSR_DSPM_RESULT_UNSUPPORTED" value="1"/>
        <value name="CSR_DSPM_RESULT_UNAVAILABLE" value="2"/>
    </type>

    <type name="CsrDspmCpid" resource="TYPE" size="16">
        <!-- Connection point identifier -->
        <value name="CSR_DSPM_CPID_INVALID"	value="0"/>
        <value name="CSR_DSPM_CPID_SOURCE"  value="0x2000"/>
        <value name="CSR_DSPM_CPID_SINK"    value="0xA000"/>
    </type>

    <type name="CsrDspmCid" resource="TYPE" size="16">
        <!-- Connection identifier -->
        <value name="CSR_DSPM_CID_INVALID"	value="0"/>
    </type>

    <type name="CsrDspmOpid" resource="TYPE" size="16">
        <!-- Operator identifier -->
        <value name="CSR_DSPM_OPID_INVALID"	value="0"/>
    </type>

    <type name="CsrDspmCpidOperator" resource="TYPE" type="CsrDspmCpid" const="1">
        <!-- Macros for extracting a connection point identifier from a given operator
             identifier and connection point number -->
        <value name="CSR_DSPM_CPID_OPERATOR_SOURCE(opid, cpn)" value="((opid) + (cpn) + CSR_DSPM_CPID_SOURCE)"/>
        <value name="CSR_DSPM_CPID_OPERATOR_SINK(opid, cpn)" value="((opid) + (cpn) + CSR_DSPM_CPID_SINK)"/>
    </type>

    <type name="CsrDspmCpidIs" resource="TYPE" const="1" type="CsrBool">
        <!-- Macros for testing whether a given cpid is a source or a sink -->
        <value name="CSR_DSPM_CPID_IS_SOURCE(cpid)" value="(((cpid) &amp; 0xA000) == CSR_DSPM_CPID_SOURCE)"/>
        <value name="CSR_DSPM_CPID_IS_SINK(cpid)" value="(((cpid) &amp; 0xA000) == CSR_DSPM_CPID_SINK)"/>
    </type>

    <type name="CsrDspmCpType" resource="TYPE" size="16">
        <!-- Connection Point Type -->
        <value name="CSR_DSPM_CP_TYPE_PCM"      value="1"/>
        <value name="CSR_DSPM_CP_TYPE_I2S"      value="2"/>
        <value name="CSR_DSPM_CP_TYPE_CODEC"    value="3"/>
        <value name="CSR_DSPM_CP_TYPE_FM"       value="4"/>
        <value name="CSR_DSPM_CP_TYPE_SPDIF"    value="5"/>
        <value name="CSR_DSPM_CP_TYPE_DM"       value="6"/>
        <value name="CSR_DSPM_CP_TYPE_A2DP"     value="7"/>
        <value name="CSR_DSPM_CP_TYPE_FASTPIPE" value="8"/>
        <value name="CSR_DSPM_CP_TYPE_SCO"      value="9"/>
    </type>

    <type name="CsrDspmCapability" resource="TYPE" size="16">
        <!-- Capabilities for use in CSR_DSPM_OPERATOR_CREATE_REQ -->
        <value name="CSR_DSPM_CAPABILITY_INVALID"               value="0x0000"/>
        <value name="CSR_DSPM_CAPABILITY_MONO_PASSTHROUGH"      value="0x0001"/>
        <value name="CSR_DSPM_CAPABILITY_STEREO_PASSTHROUGH"    value="0x0002"/>
        <value name="CSR_DSPM_CAPABILITY_SCO_SEND"              value="0x0003"/>
        <value name="CSR_DSPM_CAPABILITY_SCO_RECEIVE"           value="0x0004"/>
        <value name="CSR_DSPM_CAPABILITY_WBS_ENCODER"           value="0x0005"/>
        <value name="CSR_DSPM_CAPABILITY_WBS_DECODER"           value="0x0006"/>
        <value name="CSR_DSPM_CAPABILITY_SBC_ENCODER"           value="0x0007"/>
        <value name="CSR_DSPM_CAPABILITY_MP3_ENCODER"           value="0x0008"/>
        <value name="CSR_DSPM_CAPABILITY_RESAMPLER"             value="0x0009"/>
        <value name="CSR_DSPM_CAPABILITY_MIXER"                 value="0x000A"/>
        <value name="CSR_DSPM_CAPABILITY_APTX_ENCODER"          value="0x000B"/>
        <value name="CSR_DSPM_CAPABILITY_FM_ENHANCEMENT"        value="0x000C"/>
    </type>

    <type name="CsrDspmCpInfo" resource="TYPE">
        <!-- Info structure describing a connection point -->
        <field name="cpid"><type>CsrDspmCpid</type></field>
        <field name="cpType"><type>CsrDspmCpType</type></field>
        <field name="instance"><type>CsrUint16</type></field>
        <field name="channel"><type>CsrUint16</type></field>
    </type>

    <type name="CsrDspmConfigInfo" resource="TYPE">
        <!-- Info structure describing a connection point configuration set -->
        <field name="cpid"><type>CsrDspmCpid</type></field>
        <field name="feature"><type>CsrUint16</type></field>
        <field name="value"><type>CsrUint32</type></field>
    </type>

    <type name="CsrDspmSyncInfo" resource="TYPE">
        <!-- Info structure describing a pair of cpid's for a sync operation -->
        <field name="cpid1"><type>CsrDspmCpid</type></field>
        <field name="cpid2"><type>CsrDspmCpid</type></field>
    </type>

    <type name="CsrDspmOperatorInfo" resource="TYPE">
        <!-- Info structure describing an operator -->
        <field name="opid"><type>CsrDspmOpid</type></field>
        <field name="capability"><type>CsrDspmCapability</type></field>
    </type>

    <type name="CsrDspmOperatorMessageInfo" resource="TYPE">
        <!-- Info structure describing an operator message -->
        <field name="opid"><type>CsrDspmOpid</type></field>
        <field name="messageLength"><type>CsrUint16</type></field>
        <field name="message" blocklength="messageLength"><type>CsrUint16</type></field>
    </type>

    <type name="CsrDspmConnectionInfo" resource="TYPE">
        <!-- Info structure describing a connection -->
        <field name="cid"><type>CsrDspmCid</type></field>
        <field name="cpidSource"><type>CsrDspmCpid</type></field>
        <field name="cpidSink"><type>CsrDspmCpid</type></field>
    </type>

    <type name="CsrDspmActivateReq" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmActivateReq">
        <field name="qid">
            <!-- Application queue -->
            <type>CsrSchedQid</type>
        </field>
    </type>

    <type name="CsrDspmActivateCfm" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmActivateCfm">
    </type>

    <type name="CsrDspmDeactivateReq" resource="CSR_DSPM_SIGNALS" dvalue="CsrDspmDeactivateReq">
        <field name="qid">
