    <type name="CsrFsalSession" resource="TYPE" size="32"/>

    <type name="CsrFsalSize" resource="CONST" size="32">
        <value name="CSR_FSAL_MAX_DATA_SIZE" value="0x10000">
            <!-- Maximum allowed number of bytes to read/write in a single operation -->
        </value>
    </type>

    <type name="CsrFsalHandle" resource="TYPE" type="void*">
        <value name="CSR_FSAL_INVALID_HANDLE" value="NULL"/>
    </type>

    <type name="CsrFsalFileOpenFlags" resource="TYPE" size="16">
        <!-- File open flags (NB: only raw file access mode is supported) -->
        <value name="CSR_FSAL_OPEN_FLAGS_CREATE"                value="0x0001"><!-- Set this flag if file should be created --></value>
        <value name="CSR_FSAL_OPEN_FLAGS_READ_ONLY"             value="0x0002"><!-- Set this flag if file should be opened as read only (if set this flag takes precedence over all other flags) --></value>
        <value name="CSR_FSAL_OPEN_FLAGS_WRITE_ONLY"            value="0x0004"><!-- Set this flag if file should be opened as write only --></value>
        <value name="CSR_FSAL_OPEN_FLAGS_READ_WRITE"            value="0x0008"><!-- Set this flag if file should be opened for both read and write --></value>
        <value name="CSR_FSAL_OPEN_FLAGS_APPEND"                value="0x0010"><!-- Set this flag if file should be opened for append (if set the file pointer will be repositioned to the end of the file before every write operation) --></value>
        <value name="CSR_FSAL_OPEN_FLAGS_TRUNCATE"              value="0x0020"><!-- Set this flag if file should be truncated when opened --></value>
        <value name="CSR_FSAL_OPEN_FLAGS_EXCL"                  value="0x0040"><!-- Set this flag if file should not be created when it already exists --></value>
    </type>

    <type name="CsrFsalFileOpenPerms" resource="TYPE" size="16">
        <!-- File open permissions (only used if CSR_FSAL_OPEN_FLAGS_CREATE is set) -->
        <value name="CSR_FSAL_OPEN_PERMS_NOT_APPLICABLE"        value="0x0000"><!-- This flag should be used when the open perms is not applicable ie. when the CSR_FSAL_OPEN_FLAGS_CREATE flag is not used --></value>
        <value name="CSR_FSAL_OPEN_PERMS_READ"                  value="0x0001"><!-- Set this flag if reads are permitted --></value>
        <value name="CSR_FSAL_OPEN_PERMS_WRITE"                 value="0x0002"><!-- Set this flag if writes are permitted --></value>
    </type>

    <type name="CsrFsalMode" resource="TYPE" size="16">
        <!-- File/directory mode properties -->
        <value name="CSR_FSAL_MODE_DIRECTORY"                   value="0x0001"/>
        <value name="CSR_FSAL_MODE_REGULAR_FILE"                value="0x0002"/>
        <value name="CSR_FSAL_MODE_USER_READ_PERMISSION"        value="0x0004"/>
        <value name="CSR_FSAL_MODE_USER_WRITE_PERMISSION"       value="0x0008"/>
        <value name="CSR_FSAL_MODE_USER_EXECUTE"                value="0x0010"/>
        <value name="CSR_FSAL_MODE_GROUP_READ_PERMISSION"       value="0x0020"/>
        <value name="CSR_FSAL_MODE_GROUP_WRITE_PERMISSION"      value="0x0040"/>
        <value name="CSR_FSAL_MODE_GROUP_EXECUTE"               value="0x0080"/>
        <value name="CSR_FSAL_MODE_OTHERS_READ_PERMISSION"      value="0x0100"/>
        <value name="CSR_FSAL_MODE_OTHERS_WRITE_PERMISSION"     value="0x0200"/>
        <value name="CSR_FSAL_MODE_OTHERS_EXECUTE"              value="0x0400"/>
    </type>

    <type name="CsrFsalPermissions" resource="TYPE" size="16">
        <value name="CSR_FSAL_PERMISSION_USER_READ"             value="0x0001"><!-- Set this flag if reads are permitted for user --></value>
        <value name="CSR_FSAL_PERMISSION_USER_WRITE"            value="0x0002"><!-- Set this flag if writes are permitted for user --></value>
        <value name="CSR_FSAL_PERMISSION_USER_EXECUTE"          value="0x0004"><!-- Set this flag if execute are permitted for user --></value>
        <value name="CSR_FSAL_PERMISSION_GROUP_READ"            value="0x0008"><!-- Set this flag if reads are permitted for group --></value>
        <value name="CSR_FSAL_PERMISSION_GROUP_WRITE"           value="0x0010"><!-- Set this flag if writes are permitted for group --></value>
        <value name="CSR_FSAL_PERMISSION_GROUP_EXECUTE"         value="0x0020"><!-- Set this flag if execute are permitted for group --></value>
        <value name="CSR_FSAL_PERMISSION_OTHERS_READ"           value="0x0040"><!-- Set this flag if reads are permitted for others --></value>
        <value name="CSR_FSAL_PERMISSION_OTHERS_WRITE"          value="0x0080"><!-- Set this flag if writes are permitted for others --></value>
        <value name="CSR_FSAL_PERMISSION_OTHERS_EXECUTE"        value="0x0100"><!-- Set this flag if execute are permitted for others --></value>
    </type>

    <type name="CsrFsalOrigin" resource="TYPE" size="8">
        <!--  File seek origins -->
        <value name="CSR_FSAL_SEEK_SET"                         value="0"><!-- Beginning of file --></value>
        <value name="CSR_FSAL_SEEK_CUR"                         value="1"><!-- Current position in the file --></value>
        <value name="CSR_FSAL_SEEK_END"                         value="2"><!-- End of file --></value>
    </type>

    <type name="CsrFsalResult1" resource="TYPE" type="CsrResult" const="1">
        <!-- All file operations (open/close/read/write/seek/tell) use these error codes,
             if operation succeeded the result is CSR_RESULT_SUCCESS, all other values are reserved for future usage -->
        <value name="CSR_FSAL_FILE_OP_FAILURE"                  value="1"/>
        <value name="CSR_FSAL_FILE_OP_EOF"                      value="2"/>
        <value name="CSR_FSAL_FILE_OP_READ_ONLY"                value="3"/>
        <value name="CSR_FSAL_FILE_OP_NOT_EXIST"                value="4"/>
        <value name="CSR_FSAL_FILE_OP_NOT_ALLOWED"              value="5"/>
        <value name="CSR_FSAL_FILE_OP_ALREAD_EXISTS"            value="6"/>
        <value name="CSR_FSAL_FILE_OP_NO_SPACE"                 value="7"/>
    </type>

    <type name="CsrFsalResult2" resource="TYPE" type="CsrResult" const="1">
