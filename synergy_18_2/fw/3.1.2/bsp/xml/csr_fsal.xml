            <!-- Minutes: 0-59 -->
        </field>
        <field name="tm_hour">
            <type>CsrTime</type>
            <!-- Hours since midnight: 0-23 -->
        </field>
        <field name="tm_mday">
            <type>CsrTime</type>
            <!-- Day of the month: 1-31 -->
        </field>
        <field name="tm_mon">
            <type>CsrTime</type>
            <!-- Months *since* january: 0-11 -->
        </field>
        <field name="tm_year">
            <type>CsrTime</type>
            <!-- Years since 1900 -->
        </field>
        <field name="tm_wday">
            <type>CsrTime</type>
            <!-- Days since Sunday (0-6) -->
        </field>
        <field name="tm_yday">
            <!-- Days since Jan. 1: 0-365 -->
            <type>CsrTime</type>
        </field>
        <field name="tm_isdst">
            <!-- +1 Daylight Savings Time, 0 No DST, 0xFFFF don't know -->
            <type>CsrTime</type>
        </field>
        <field name="utcTime">
            <!-- TRUE if given in UTC time, FALSE if given in local time  -->
            <type>CsrBool</type>
        </field>
    </type>

    <type name="CsrFsalDirEntry" resource="TYPE">
        <field name="name">
            <!-- Name of entry -->
            <type>CsrUtf8String*</type>
        </field>
        <field name="time">
            <!-- Time of last write/modification of entry -->
            <type>CsrFsalTm</type>
        </field>
        <field name="size">
            <!-- 0 if not file -->
            <type>CsrSize</type>
        </field>
        <field name="mode">
            <!-- mode -->
            <type>CsrFsalMode</type>
        </field>
    </type>

</definitions>
